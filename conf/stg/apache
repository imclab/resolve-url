<VirtualHost *:80>
    ServerName resolve-url.knilab.com
    ServerAdmin admin@knilab.com
    ErrorLog /var/log/apache2/resolve-url.log

    LogLevel info

    SetEnvIf X-Forwarded-Protocol "^https$" HTTPS=on

    WSGIDaemonProcess resolve-url display-name=%{GROUP} maximum-requests=10000
    WSGIProcessGroup resolve-url
    WSGIScriptAlias / /home/apps/sites/resolve-url/conf/stg/wsgi.py
</VirtualHost>
